        }
    ],
    "require": {
        "php": ">=7.4",
        "eftec/bladeone": "^4.7"
    },
    "bin": [
        "stormphp"
    ],
    "autoload": {
        "psr-4": {
            "App\\Controllers\\": "app/controllers/",
            "App\\Models\\": "app/models/",
            "Core\\": "app/core/",
            "App\\Routes\\": "routes/"
        }
    }
}
